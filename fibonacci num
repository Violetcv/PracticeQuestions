/*Fibonacci Number*/
#include<stdio.h>

int fib(int num){
    if (num==2||num==1){
        return 1;
    }   
    else {
        return fib(num-1)+fib(num-2);
    }    
}

int main(){
    int num;
    printf("Enter the Number: ");
    scanf("%d", &num);
    printf("The %d Fibonacci number is %d", num, fib(num));
    return 0;
}
